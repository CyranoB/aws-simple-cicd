    "@aws-cdk/aws-codepipeline-actions": "^1.100.0",
    "@aws-cdk/aws-iam": "^1.100.0",
    "@aws-cdk/aws-lambda": "^1.100.0",
    "@aws-cdk/aws-s3": "^1.100.0",
    "@aws-cdk/aws-sam": "^1.100.0",
    "@aws-cdk/core": "^1.100.0",
    "@types/node": "^14.14.41",
    "aws-cdk": "^1.100.0",
    "aws-sdk": "^2.890.0",
    "husky": "^6.0.0",
    "tsort": "0.0.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@aws-cdk/aws-s3-deployment": "^1.100.0",
    "@aws-cdk/aws-ssm": "^1.100.0",
